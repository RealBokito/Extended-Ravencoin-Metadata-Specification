{
    "schema":"ravencoin_nft",
    "version": 0.1,
    "name": {
        "required":1,
        "type": "string",
        "description": "The name of the NFT/Asset."
    },
    "description": {
        "required":1,
        "type": "string",
        "description": "Description of the NFT/Asset."
    },
    "image": {
        "required":1,
        "type": "string",
        "description": "A URI to a resource which represents the NFT/Asset."
    },
    "animation_uri": {
        "required":0,
        "type": "string",
        "description": "Optional URI to a animation/video of the NFT/Asset."
    },
    "external_uri": {
        "required":0,
        "type": "string",
        "description": "Optional URI to a external resource, webpage or website related to the NFT/Asset."
    },
    "attributes": {
        "required": 0,
        "type": "json",
        "description": "Optional JSON object that defines a set of attributes which classifies and defines the NFT."
    },
    "contact_name": {
        "required":1,
        "type": "string"
    },    
    "contact_company_name": {
        "required":0,
        "type": "string"
    },
    "contact_email": {
        "required":1,
        "type": "string"
    },
    "contact_address": {
		"street_address1": {
			"required":1,
			"type": "string"
		},
		"street_address2": {
			"required":0,
			"type": "string"
		},
		"city": {
			"required":1,
			"type": "string"
		},
		"state": {
			"required":0,
			"type": "string"
		},
		"postal_code": {
			"required":1,
			"type": "string"
		},
		"country": {
			"required":1,
			"type": "string"
		}		
	},
    "contact_phone": {
        "required":0,
        "type": "string"
    }
}
